#include <iostream>
#include <conio.h>

using namespace std;

int invertir(int numero)
{
	int invertido = 0;
	while (numero != 0)
	{
		invertido = invertido * 10 + numero % 10;
		numero = numero / 10;
	}
	return invertido;
}
int main()
{
	int numero;
	cout << "Ingrese el numero: "; 
	cin >> numero;
	cout << invertir(numero);

	_getch();
}
